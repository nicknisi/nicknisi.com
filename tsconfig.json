{
  "extends": "astro/tsconfigs/strictest",
  "compilerOptions": {
    "types": ["vite/client"],
    "baseUrl": ".",
    "paths": {
      "@components/*": [
        "src/components/*"
      ],
      "@layouts/*": [
        "src/layouts/*"
      ],
      "@data/*": [
        "src/data/*"
      ]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}
